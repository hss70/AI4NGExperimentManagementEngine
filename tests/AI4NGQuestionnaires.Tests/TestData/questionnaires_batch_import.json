    {
      "id": "IPAQ",
      "data": {
        "name": "International Physical Activity Questionnaire - Short Form",
        "description": "Assesses physical activity levels over the past 7 days",
        "estimatedTime": 180,
        "version": "1.0",
        "questions": [
