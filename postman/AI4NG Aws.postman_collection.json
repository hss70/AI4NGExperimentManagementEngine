					"name": "GetToken",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse the response body\r",
									"let responseJson = pm.response.json();\r",
									"\r",
									"// Check if AuthenticationResult is present\r",
									"if (responseJson.AuthenticationResult) {\r",
									"    let authResult = responseJson.AuthenticationResult;\r",
									"\r",
									"    // Set environment variables\r",
									"    pm.environment.set(\"accessToken\", authResult.AccessToken);\r",
									"    pm.environment.set(\"idToken\", authResult.IdToken);\r",
									"    pm.environment.set(\"refreshToken\", authResult.RefreshToken);\r",
									"\r",
									"    // Calculate expiry time in milliseconds\r",
									"    let expiryTime = new Date().getTime() + (authResult.ExpiresIn * 1000);\r",
									"    pm.environment.set(\"accessTokenExpiry\", expiryTime);\r",
									"\r",
									"    console.log(\"Tokens set successfully in environment variables.\");\r",
									"} else if (responseJson.ChallengeName == \"NEW_PASSWORD_REQUIRED\")\r",
									"{\r",
									"    pm.environment.set(\"authSessionId\", responseJson.Session);\r",
									"}\r",
									"else {\r",
									"    console.log(\"AuthenticationResult not found in response.\");\r",
									"}"
								],
