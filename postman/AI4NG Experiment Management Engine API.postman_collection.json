							{
								"key": "Authorization",
								"value": "Bearer {{jwt}}",
								"type": "text"
							}
						],
						"url": "{{baseUrl}}/api/questionnaires/{{questionnaireId}}",
						"description": "Delete a questionnaire (soft delete)."
					}
				}
			]
		}
	],
